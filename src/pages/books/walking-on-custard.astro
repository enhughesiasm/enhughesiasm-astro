---
import Woc from '../../components/books/woc/Woc.astro';

import wocCover from '../../assets/images/wocCover.jpg';
import BaseLayout from '../../layouts/BaseLayout.astro';
import LayoutBook from '../../layouts/Layout_Book.astro';
import { SEO } from 'astro-seo';
---

<BaseLayout>
	<Fragment slot='seo'>
		<SEO
			title={'Walking on Custard - The Best Funny Book about Anxiety - Walking on Custard'}
		/>
	</Fragment>
	<LayoutBook
		key='walking-on-custard'
		title='Walking on Custard & the Meaning of Life'
		linkUrl='/books/walking-on-custard'
		coverImg={wocCover}
	>
		<Woc />
	</LayoutBook>
</BaseLayout>
